{
  "functions": {
    "app/api/admin/generate-video/**": {
      "maxDuration": 300
    },
    "app/api/admin/generate-completion-video/**": {
      "maxDuration": 300
    },
    "app/api/admin/generate-map-video/**": {
      "maxDuration": 300
    },
    "app/api/admin/journey/generate-video/**": {
      "maxDuration": 300
    },
    "app/api/admin/generate-media/**": {
      "maxDuration": 300
    },
    "app/api/admin/generate-bulk-media/**": {
      "maxDuration": 300
    },
    "app/api/admin/generate-image/**": {
      "maxDuration": 60
    },
    "app/api/admin/generate-bulk-images/**": {
      "maxDuration": 300
    },
    "app/api/admin/generate-audio/**": {
      "maxDuration": 300
    },
    "app/api/admin/journey/generate-audio/**": {
      "maxDuration": 300
    },
    "app/api/tts/**": {
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}

